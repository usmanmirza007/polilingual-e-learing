{
  "name": "eduma",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "npx jetify",
    "a": "react-native run-android",
    "ip12": "npx react-native run-ios --simulator='iPhone 12'",
    "ip13": "npx react-native run-ios --simulator='iPhone 13'",
    "ip14": "npx react-native run-ios --simulator='iPhone 14'",
    "ipad": "npx react-native run-ios --simulator='iPad (8th generation)'",
    "pod": "cd ios && pod install && cd ..",
    "iX": "react-native run-ios --simulator=\"iPhone X\"",
    "i8": "react-native run-ios --simulator=\"iPhone 8\"",
    "dip": "react-native run-ios --device",
    "oppo": "react-native run-android --deviceId=ad42f1",
    "clean-a": "cd android && ./gradlew clean && cd ..",
    "bundle-a": "cd android && ./gradlew bundleRelease && cd ..",
    "reverse": "adb reverse tcp:8081 tcp:8081",
    "prelease": "sh release",
    "build-a": "cd android && ./gradlew assembleRelease && cd ..",
    "release-a": "yarn prelease && android/gradlew assembleRelease",
    "shake-a": "adb shell input keyevent 82",
    "beta-ios": "cd ios && bundle exec fastlane beta && cd ..",
    "beta-android": "cd android && bundle exec fastlane beta && cd ..",
    "beta": "yarn beta-ios && yarn beta-android",
    "release-ios": "cd ios && bundle exec fastlane release && cd ..",
    "release-android": "cd android && bundle exec fastlane release && cd ..",
    "release": "yarn release-ios && yarn release-android"
  },
  "dependencies": {
    "@native-html/iframe-plugin": "^2.6.1",
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.5",
    "i18next": "^21.6.16",
    "jwt-decode": "^3.1.2",
    "moment": "^2.27.0",
    "native-base": "^2.13.12",
    "querystringify": "^2.2.0",
    "react": "17.0.2",
    "react-i18next": "^11.16.7",
    "react-native": "0.64.4",
    "react-native-animatable": "^1.3.3",
    "react-native-background-timer": "^2.4.1",
    "react-native-code-push": "^6.3.0",
    "react-native-collapsible": "^1.6.0",
    "react-native-device-info": "^5.6.1",
    "react-native-document-picker": "^6.0.4",
    "react-native-draggable-flatlist": "^2.6.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.18.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-iap": "7.5.6",
    "react-native-image-crop-picker": "^0.37.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^11.7.0",
    "react-native-ratings": "^7.6.1",
    "react-native-reanimated": "^2.0.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1",
    "react-native-sortable-list": "^0.0.24",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.12.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^11.23.1",
    "react-navigation": "^4.4.4",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-navigation-stack": "^2.10.4",
    "react-navigation-tabs": "^2.11.0",
    "react-redux": "^7.2.3",
    "realm": "10.21.1",
    "redux": "^4.0.5",
    "redux-form": "^8.3.7",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "rn-placeholder": "^3.0.3",
    "yarn": "1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jetifier": "^1.6.6",
    "lint-staged": "^10.4.0",
    "metro-react-native-babel-preset": "^0.63.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^1.11.1",
    "react-test-renderer": "17.0.2",
    "reactotron-react-native": "^5.0.3"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!static-container|jest).+\\.js$"
    ],
    "modulePathIgnorePatterns": [
      "node_modules/(?!static-container|jest).+\\.js$",
      "<rootDir>/.*/__mocks__"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "exclude": [
      "**/__mocks__/*",
      "../../node_modules",
      "build",
      "src/**/*.test.ts",
      "src/**/*.test.tsx"
    ]
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/Bruno Ace Regular",
      "./src/assets/fonts/Poppins",
      "./src/assets/fonts/Sniglet"
    ]
  }
}
